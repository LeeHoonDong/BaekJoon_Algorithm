count=0
def dfs(i,result,numbers,target,length):
    global count
    if i==length:
        if result==target:
            count+=1
        return
    dfs(i+1,result+numbers[i],numbers,target,length)
    dfs(i+1,result-numbers[i],numbers,target,length)

def solution(numbers, target):
    global count
    dfs(0,0,numbers,target,len(numbers))
    return count